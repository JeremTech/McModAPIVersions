<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Authors>JeremTech</Authors>
    <Description>Allow to retrieve Forge and Fabric API informations like available versions or download links.</Description>
    <PackageTags>Minecraft, API, Versions, Forge, Fabric, Mod</PackageTags>
    <PackageProjectUrl>https://github.com/JeremTech/McModAPIVersions</PackageProjectUrl>
    <Version>1.1.0</Version>
    <PackageReleaseNotes>1.1.0 :
- Added function to build Forge MDK download link
- Added function to build Forge Installer download link
- Updated package informations</PackageReleaseNotes>
    <PackageLicenseExpression>GPL-3.0 License</PackageLicenseExpression>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
    <DocumentationFile>D:\C#\Libs\McModAPIVersions\McModAPIVersions\McModAPIVersions\McModAPIVersions.xml</DocumentationFile>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json" Version="12.0.3" />
  </ItemGroup>

</Project>
